  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:packages": "turbo run build --filter=@edgeloop/shared --filter=@edgeloop/server --filter=@edgeloop/ui --filter=@edgeloop/ml",
    "start": "turbo run start",
    "typecheck": "turbo run typecheck",
    "check": "turbo run typecheck",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test --coverage",
    "test:ui": "vitest --ui",
    "clean": "turbo run clean",
    "pre-deploy": "pnpm run build:packages && pnpm run lint && pnpm run typecheck && pnpm run test",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "env:doctor": "tsx scripts/env-doctor.ts",
    "env:doctor:json": "tsx scripts/env-doctor.ts --json",
    "validate:contracts": "echo 'Contract validation - implement schema validation'",
    "validate:production": "tsx scripts/validate-production.ts",
    "vercel:ops": "tsx scripts/vercel-ops.ts",
    "vercel:integrations": "tsx scripts/vercel-ops.ts integrations:list",
    "vercel:logs": "tsx scripts/vercel-ops.ts logs:deployment",
    "vercel:project": "tsx scripts/vercel-ops.ts projects:get",
    "vercel:projects": "tsx scripts/vercel-ops.ts projects:list",
    "vercel:teams": "tsx scripts/vercel-ops.ts teams:list",
    "vercel:firewall": "tsx scripts/vercel-ops.ts security:firewall",
    "vercel:rolling-release": "tsx scripts/vercel-ops.ts rolling-release:config",
    "vercel:access-groups": "tsx scripts/vercel-ops.ts access-groups:list",
    "approve-builds": "pnpm config set auto-install-peers true"
  },
